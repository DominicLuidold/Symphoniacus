<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.2"
             xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd">
    <!-- Persistence unit -->
    <persistence-unit name="mysqldb">
        <!-- Entity classes -->
        <class>at.fhv.teamb.symphoniacus.persistence.model.AdministrativeAssistantEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.ContractualObligationEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.DutyCategoryChangelogEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.DutyCategoryEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.DutyEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.DutyPositionEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.InstrumentationEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.InstrumentationPositionEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.InstrumentCategoryEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.MonthlyScheduleEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.MusicalPieceEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.MusicianEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.MusicianRoleEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.NegativeDateWishEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.NegativeDutyWishEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.PositiveWishEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.SectionEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.SectionInstrumentationEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.SectionInstrumentCategory</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.SectionMonthlyScheduleEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.SeriesOfPerformancesEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.Substitute</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.UserEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.VacationEntity</class>
        <class>at.fhv.teamb.symphoniacus.persistence.model.WeeklyScheduleEntity</class>
        <!-- Properties -->
        <properties>
            <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver" />
            <property name="javax.persistence.jdbc.url" value="***REMOVED***" />
            <property name="javax.persistence.jdbc.user" value="***REMOVED***" />
            <property name="javax.persistence.jdbc.password" value="***REMOVED***" />

            <property name="hibernate.dialect" value="org.hibernate.dialect.MySQLDialect" />
            <property name="hibernate.show_sql" value="true" />
            <property name="hibernate.format_sql" value="true" />
        </properties>
    </persistence-unit>
</persistence>